{
  "routes": [
    {
      "route": "/*",
      "allowedRoles":  ["authenticated"]
    },
    {
      "route": "/counter",
      "allowedRoles": [ "owner" ]
    },
    {
      "route": "/quizcard",
      "allowedRoles": [ "authenticated" ]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "/static/*", "/api/*" ]
  },
  "responseOverrides": {
    "401": {
      "statusCode": 302,
      "redirect": "/.auth/login/aad"
    }
  }
}
